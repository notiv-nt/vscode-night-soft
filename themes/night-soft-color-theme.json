  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#676767"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.colora", "meta.object-literal.key"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "keyword.control", "storage.type.class", "storage.modifier", "punctuation"],
      "settings": {
        "foreground": "#FF79C6"
      }
    },
    {
      "name": "Typescript Cast",
      "scope": ["cast.expr.ts"],
      "settings": {
        "foreground": "#FF79C6",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.css",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag.html",
        "entity.name.tag.svelte",
        "invalid.illegal.unrecognized-tag.html",
        "support.class.component.js",
        "support.class.component.tsx",
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "entity.name.tag.template.html",
        "entity.name.tag.script.html",
        "entity.name.tag.style.html",
        "entity.name.tag.inline.any.html",
        "entity.name.tag.block.any.html",
        "support.class.component.html",
        "entity.name.tag.other.html",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "entity.name.tag.css",
        "support.type.property-name.json",
        "entity.name.tag.yaml",
        "support.class.component.svelte",
        "entity.name.tag.i18n.html.vue",
        "meta.selector.css"
      ],
      "settings": {
        "foreground": "#e8276d"
      }
    },
    {
      "name": "Invalid unrecognized tag",
      "scope": ["invalid.illegal.unrecognized-tag.html"],
      "settings": {
        "fontStyle": "italic"
      }
    },

    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name",
        "entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.single.html",
        "string.quoted.double.html",
        "string.unquoted.html",
        "support.function",
        "entity.name.function",
        "string.quoted.double.json",
        "string.quoted.single.yaml",
        "string.quoted.double.yaml",
        "string.unquoted.plain.in.yaml",
        "string.unquoted.plain.out.yaml",
        "punctuation.definition.template-expression.begin.js",
        "punctuation.definition.template-expression.end.js"
      ],
      "settings": {
        "foreground": "#61FF8E"
      }
    },

    {
      "name": "Strings",
      "scope": ["string.quoted", "string.regexp", "string.template", "meta.attribute-selector.scss"],
      "settings": {
        "foreground": "#B0DB65"
      }
    },

    {
      "name": "Strings",
      "scope": ["storage.type", "support.variable", "support.class", "support.class.builtin", "support.type.object.module"],
      "settings": {
        "foreground": "#8BE9FD",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Strings",
      "scope": ["constant.numeric", "constant.language", "variable.language.this"],
      "settings": {
        "foreground": "#BD93F9"
      }
    },

    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
