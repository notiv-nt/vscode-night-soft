{
  "name": "night-soft",
  "type": "dark",
  "colors": {
    "editor.background": "#0D121F",
    "editor.foreground": "#eeffff",

    "sideBarTitle.foreground": "#eee",
    "sideBar.background": "#090d17",
    "editorSuggestWidget.background": "#091118",
    "list.activeSelectionBackground": "#232b2e",
    "list.hoverBackground": "#181e1f",
    "quickInput.background": "#000",

    "activityBarBadge.background": "#007acc",
    "activityBar.background": "#05080F",
    "titleBar.activeBackground": "#05080F",
    "titleBar.inactiveBackground": "#0A0C0D",
    "titleBar.activeForeground": "#FFFFFF",
    "titleBar.inactiveForeground": "#AAAAAA",

    "badge.background": "#0099FF",
    "badge.foreground": "#FFFFFF",

    "button.background": "#0099FF",
    "button.foreground": "#fff",
    "dropdown.background": "#0D1011",
    "dropdown.border": "#333",
    "input.background": "#0D1011",
    "input.border": "#333",

    "tab.activeBackground": "#111213",
    "tab.activeForeground": "#CBCBCC",
    "tab.activeModifiedBorder": "#09f",
    "tab.activeBorder": "#09f",
    "tab.border": "#000",
    "tab.hoverBackground": "#111213",
    "tab.inactiveBackground": "#1F2326",
    "tab.inactiveForeground": "#D4D7D6",
    "editorGroupHeader.tabsBackground": "#181D22",
    "editorGroupHeader.tabsBorder": "#000",

    "statusBar.background": "#0D1011",
    "statusBar.foreground": "#AAAAAA",

    "editor.lineHighlightBackground": "#1D1F27",
    "editor.lineHighlightBorder": "#2D2F37",

    "selection.background": "#cce",
    "editor.selectionBackground": "#2e364c",
    "editor.selectionHighlightBackground": "#0000",
    "editor.selectionHighlightBorder": "#2d5074",
    "editorCursor.foreground": "#fff",

    "editorIndentGuide.activeBackground": "#6a6b72"
  },

  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#676767"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "string constant.other.placeholder"],
      "settings": {
        "foreground": "#fff"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.colora", "meta.object-literal.key"],
      "settings": {
        "foreground": "#ffffff"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FF5370"
      }
    },
    {
      "name": "Keyword, Storage",
      "scope": ["keyword", "keyword.control", "storage.type.class", "storage.modifier", "punctuation"],
      "settings": {
        "foreground": "#FF79C6"
      }
    },
    {
      "name": "Operator, Misc",
      "scope": [
        "constant.other.color",
        "punctuation",
        "meta.tag",
        "punctuation.definition.tag",
        "punctuation.separator.inheritance.php",
        "punctuation.definition.tag.html",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html",
        "punctuation.definition.css",
        "punctuation.section.embedded",
        "keyword.other.template",
        "keyword.other.substitution"
      ],
      "settings": {
        "foreground": "#FFFFFF"
      }
    },
    {
      "name": "Tag",
      "scope": [
        "entity.name.tag.html",
        "support.class.component.js",
        "support.class.component.tsx",
        "entity.name.tag.js",
        "entity.name.tag.tsx",
        "entity.name.tag.template.html",
        "entity.name.tag.script.html",
        "entity.name.tag.style.html",
        "entity.name.tag.inline.any.html",
        "entity.name.tag.block.any.html",
        "entity.name.tag.other.html",
        "meta.tag.sgml",
        "markup.deleted.git_gutter",
        "entity.name.tag.css",
        "support.type.property-name.json",
        "entity.name.tag.yaml",
        "support.class.component.svelte"
      ],
      "settings": {
        "foreground": "#e8276d"
      }
    },

    {
      "name": "HTML Attributes",
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.js",
        "entity.other.attribute-name.tsx",
        "entity.other.attribute-name",
        "entity.other.inherited-class"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string.quoted.single.html",
        "string.quoted.double.html",
        "string.unquoted.html",
        "support.function",
        "entity.name.function",
        "string.quoted.double.json",
        "string.quoted.single.yaml",
        "string.quoted.double.yaml",
        "string.unquoted.plain.in.yaml",
        "string.unquoted.plain.out.yaml"
      ],
      "settings": {
        "foreground": "#61FF8E"
      }
    },

    {
      "name": "Strings",
      "scope": ["string.quoted", "string.regexp", "string.template", "meta.attribute-selector.scss"],
      "settings": {
        "foreground": "#B0DB65"
      }
    },

    {
      "name": "Strings",
      "scope": ["storage.type", "support.variable", "support.class", "support.class.builtin", "support.type.object.module"],
      "settings": {
        "foreground": "#8BE9FD",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Strings",
      "scope": ["constant.numeric", "constant.language", "variable.language.this"],
      "settings": {
        "foreground": "#BD93F9"
      }
    },

    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#FFB86C"
      }
    },

    {
      "name": "Class super()",
      "scope": ["variable.language.super"],
      "settings": {
        "foreground": "#ff38ee",
        "fontStyle": "italic"
      }
    },

    {
      "name": "Class name",
      "scope": ["entity.name.type.class"],
      "settings": {
        "foreground": "#ffc038"
      }
    },

    {
      "name": "Function Names",
      "scope": ["entity.name.type", "variable.other.constant.object", "support.function.js", "support.function.tsx", "support.function"],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },

    {
      "name": "CSS class names",
      "scope": [
        "entity.other.attribute-name.class.css",
        "punctuation.definition.entity.css",
        "entity.other.attribute-name.parent-selector-suffix.css",
        "entity.other.attribute-name.attribute.css",
        "entity.other.attribute-name.pseudo-element.css"
      ],
      "settings": {
        "foreground": "#61FF8E",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS pseudo class",
      "scope": ["entity.other.attribute-name.pseudo-class.css"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#F1FA8C"
      }
    },
    {
      "name": "CSS attrs",
      "scope": ["meta.attribute-selector.css", "entity.other.attribute-name.attribute.scss", "entity.other.attribute-selector.postcss"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "CSS prop names",
      "scope": ["support.type.property-name.css", "meta.property-name.scss", "support.type.vendored.property-name.css"],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "CSS colors",
      "scope": ["constant.other.color.rgb-value.hex.css", "constant.other.color.rgb-value.css.postcss", "support.type.property-name.media.css"],
      "settings": {
        "foreground": "#F1FA8C"
      }
    },
    {
      "name": "CSS variables",
      "scope": ["variable.css", "variable.argument.css", "variable.parameter.postcss", "variable.scss"],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "CSS id",
      "scope": ["entity.other.attribute-name.id.css"],
      "settings": {
        "foreground": "#FFB86C"
      }
    },
    {
      "name": "Constants",
      "scope": ["support.constant.font-name.css", "constant.other.php", "support.constant.media.css"],
      "settings": {
        "foreground": "#FFB86C"
      }
    },

    {
      "name": "Markdown - Plain",
      "scope": ["text.html.markdown", "punctuation.definition.list_item.markdown"],
      "settings": {
        "foreground": "#eff"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline",
      "scope": ["text.html.markdown markup.inline.raw.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Markup Raw Inline Punctuation",
      "scope": ["text.html.markdown markup.inline.raw.markdown punctuation.definition.raw.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Heading",
      "scope": ["markup.heading.markdown"],
      "settings": {
        "foreground": "#61FF8E"
      }
    },
    {
      "name": "Markdown - Heading Punctuation",
      "scope": ["punctuation.definition.heading.markdown"],
      "settings": {
        "foreground": "#8BE9FD"
      }
    },
    {
      "name": "Markdown - Punctuation",
      "scope": ["markup.bold.markdown", "punctuation.definition.bold.markdown"],
      "settings": {
        "foreground": "#FFB86C",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown - Italic",
      "scope": ["markup.italic.markdown", "punctuation.definition.italic.markdown"],
      "settings": {
        "foreground": "#FFB86C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown - Quote",
      "scope": ["markup.quote.markdown"],
      "settings": {
        "foreground": "#BD93F9"
      }
    },
    {
      "name": "Markdown - List",
      "scope": ["punctuation.definition.list.begin.markdown"],
      "settings": {
        "foreground": "#FF79C6"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": ["markup.inline.raw.string.markdown"],
      "settings": {
        "foreground": "#61FF8E"
      }
    },
    {
      "name": "Markdown - Code",
      "scope": ["markup.fenced_code.block.markdown"],
      "settings": {
        "foreground": "#aaa"
      }
    },
    {
      "name": "Markdown - Link",
      "scope": ["string.other.link.title.markdown"],
      "settings": {
        "foreground": "#82AAFF"
      }
    },
    {
      "name": "Markdown - Link Description",
      "scope": ["string.other.link.description.title.markdown"],
      "settings": {
        "foreground": "#C792EA"
      }
    },
    {
      "name": "Markdown - Link Anchor",
      "scope": ["constant.other.reference.link.markdown"],
      "settings": {
        "foreground": "#FFCB6B"
      }
    },
    {
      "name": "Markdown - Raw Block Fenced",
      "scope": ["markup.raw.block.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block",
      "scope": ["punctuation.definition.fenced.markdown"],
      "settings": {
        "foreground": "#00000050"
      }
    },
    {
      "name": "Markdown - Fenced Bode Block Variable",
      "scope": ["markup.raw.block.fenced.markdown", "variable.language.fenced.markdown", "punctuation.section.class.end"],
      "settings": {
        "foreground": "#EEFFFF"
      }
    },
    {
      "name": "Markdown - Fenced Language",
      "scope": ["variable.language.fenced.markdown"],
      "settings": {
        "foreground": "#65737E"
      }
    },
    {
      "name": "Markdown - Separator",
      "scope": ["meta.separator"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#65737E"
      }
    }
  ]
}
